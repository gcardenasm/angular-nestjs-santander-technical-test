{
  "name": "candidate-backend",
  "version": "1.0.0",
  "description": "NestJS 11.1.6 backend service for processing candidate Excel files.",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ormconfig.ts migration:generate src/migrations/init",
    "typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ormconfig.ts migration:run",
    "typeorm:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ormconfig.ts migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config jest-e2e.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.1.6",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.26",
    "typescript": "5.9.2"
  }
}
